{
    "manifest_version": 3,
    "name": "Joke Popup",
    "version": "1.0",
    "description": "Displays a joke in Urdu every time you click the extension icon.",
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon.png"
    }
  }
  